{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "lee-s-productivity-toolbox",
  "title": "Lee's Productivity Toolbox",
  "description": "Building an extension together with Lee during a livestream to make him more productive",
  "icon": "lee.png",
  "author": "thomas",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search",
      "title": "Search GitHub Issues and Pull Requests",
      "subtitle": "Lee",
      "description": "Search GitHub issues and pull requests",
      "mode": "view"
    },
    {
      "name": "open-issues",
      "title": "Open Issues",
      "description": "Just open issues",
      "mode": "menu-bar",
      "subtitle": "Lee",
      "icon": "lee.png",
      "interval": "15m"
    }
  ],
  "preferences": [
    {
      "name": "personalAccessToken",
      "title": "Personal Access Token",
      "description": "Personal Access Token for GitHub",
      "type": "password",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.0",
    "@raycast/utils": "^1.3.1"
  },
  "devDependencies": {
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}